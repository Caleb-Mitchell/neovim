{
	"PEDAC": {
		"prefix": "ped",
    "description": "Display pedac template",
		"body": [
			"=begin\n",
      "== Problem ==",
			"- $1\n",
      "Input: $2",
      "Output: $3\n",
      "Rules/Requirements",
      "- $4\n",
      "Mental Model",
      "- $5\n",
			"== Examples/Test Cases ==",
			"$6\n",
      "== Data Structure(s) ==",
      "- $7\n",
      "== Algorithm ==\n",
      "$8\n",
      "== Code (with intent) ==",
      "=end",
      "$9"
		]
  },
  "html-caleb": {
      "prefix": "html-caleb",
      "body": [
        "<!DOCTYPE html>",
        "<html lang=\"$1en\">",
        "\t<head>",
        "\t\t<title>$2</title>",
        "\t\t<meta charset=\"UTF-8\">",
        "\t</head>",
        "\t<body>",
        "\t$4",
        "\t</body>",
        "</html>"
      ],
      "description": "HTML - My WiP html boilterplate",
      "scope": "text.html"
  },
  "css-reset-caleb": {
      "prefix": "css-reset-caleb",
      "body": [
        "html {",
          "\tbox-sizing: border-box;",
        "}",
        "",
        "*, *::before, *::after {",
          "\tbox-sizing: inherit;",
        "}"
      ],
      "description": "CSS - My WiP css reset boilerplate",
      "scope": "text.css"
  }
}
